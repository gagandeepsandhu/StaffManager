@{
    ViewData["Title"] = "";
}

<h2>Staff Details</h2>

@{int i = 0;}
@foreach (var emp in ViewBag.display)
{

    <table border="0" cellpadding="0" cellspacing="0" style="width:10%;float:left">
        <tbody>

            @foreach (var item in emp)
            {
                <tr>
                    <td>@item</td>
                </tr>
                i++;
            }
        </tbody>
    </table>

}

    <form action="/home/panel" method="get">
        <button type="submit">GO BACK</button>
    </form>

<style>

    td, th {
        border: 1px solid #000000;
        text-align: left;
        padding: 8px;
    }

    tr:nth-child(even) {
        background-color: #FFFFFF;
    }

    button {
        background-color: #4CAF50;
        color: white;
        padding: 14px 20px;
        margin: 8px 0;
        border: none;
        cursor: pointer;
        width: 100%;
    }
</style>